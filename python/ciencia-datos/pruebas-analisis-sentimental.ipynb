    "import spacy\n",
    "from textblob import TextBlob\n",
    "from colorama import init, Fore, Back, Style\n",
    "\n",
    "# Inicializar colorama\n",
    "init()\n",
    "\n",
    "# Cargar modelo de lenguaje de Spacy\n",
    "nlp = spacy.load('es_core_news_sm')\n",
    "\n",
    "# Abrir archivo\n",
    "with open('declaracion-rio-1992.txt') as archivo:\n",
    "    # Leer contenido\n",
    "    contenido = archivo.read()\n",
    "\n",
    "# Palabras clave a buscar\n",
    "palabras_clave = ['desarrollo sostenible']\n",
    "\n",
    "# Recorrer palabras clave\n",
    "for palabra in palabras_clave:\n",
    "    # Imprimir encabezado con título de palabra clave\n",
    "    print(f\"\\n{Fore.YELLOW}{palabra.upper()}{Style.RESET_ALL}\\n\")\n",
    "    \n",
    "    # Buscar frases que contienen la palabra clave\n",
    "    doc = nlp(contenido)\n",
    "    frases = [sent.text.strip() for sent in doc.sents if palabra in sent.text.lower()]\n",
    "    \n",
    "    # Imprimir frases encontradas\n",
    "    for frase in frases:\n",
    "        # Calcular valoración de la frase\n",
    "        valoracion = TextBlob(frase).sentiment.polarity\n",
    "        \n",
    "        # Imprimir frase en color verde si tiene valoración positiva, rojo si tiene valoración negativa,\n",
    "        # amarillo si tiene valoración neutra y blanco si no tiene valoración\n",
    "        if valoracion > 0:\n",
    "            print(f\"{Fore.GREEN}{frase}{Style.RESET_ALL}\")\n",
    "        elif valoracion < 0:\n",
    "            print(f\"{Fore.RED}{frase}{Style.RESET_ALL}\")\n",
    "        elif valoracion == 0:\n",
    "            print(f\"{Fore.YELLOW}{frase}{Style.RESET_ALL}\")\n",
    "    \n",
    "    # Imprimir separador\n",
    "    print(\"\\n\" + Fore.BLUE + \"=\" * 80 + Style.RESET_ALL + \"\\n\")\n"
