    "with open('ambrosio-licona.txt') as archivo:\n",
    "with open('ambrosio-licona.md') as archivo:\n",
    "    # Leer contenido\n",
    "    contenido = archivo.read()\n",
    "\n",
    "# Palabras clave a buscar\n",
