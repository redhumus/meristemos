    "import re\n",
    "\n",
    "# Leer archivo de texto\n",
    "with open('ambrosio-licona.md', 'r') as f:\n",
    "with open('ambrosio-licona.txt', 'r') as f:\n",
    "with open('ambrosio-licona.md', 'r') as f:\n",
    "    texto = f.read()\n",
    "\n",
    "# Obtener frases\n",
    "frases = re.findall(r'[A-Z][^\\.!?]*[\\.!?]', texto)\n",
    "\n",
    "# Obtener palabras y contar frecuencia\n",
    "palabras = {}\n",
    "excluir = ['el', 'la', 'en', 'de', 'que', 'los', 'del', 'lo', 'una', 'y', 'se', 'al', 'las', 'a', 'su', 'por']\n",
    "for palabra in texto.split():\n",
    "    palabra = palabra.lower().strip('.,')\n",
    "    if palabra in excluir:\n",
    "        continue\n",
    "    if palabra in palabras:\n",
    "        palabras[palabra] += 1\n",
    "    else:\n",
    "        palabras[palabra] = 1\n",
    "\n",
    "# Ordenar palabras por frecuencia descendente\n",
    "palabras = sorted(palabras.items(), key=lambda x: x[1], reverse=True)\n",
    "\n",
    "# Imprimir frases y palabras más frecuentes\n",
    "print(\"Frases:\")\n",
    "for frase in frases:\n",
    "    print(frase)\n",
    "\n",
    "print(\"\\nPalabras más frecuentes:\")\n",
    "for palabra, frecuencia in palabras:\n",
    "    print(f\"{palabra}: {frecuencia}\")\n"
   ]
