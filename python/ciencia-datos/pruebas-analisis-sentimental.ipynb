    "import spacy\n",
    "import networkx as nx\n",
    "\n",
    "# Carga el modelo de spaCy\n",
    "nlp = spacy.load(\"es_core_news_sm\")\n",
    "\n",
    "# Define el texto a analizar\n",
    "texto = \"\"\"\n",
    "El presidente de México, Andrés Manuel López Obrador, visitó la ciudad de Nueva York para reunirse con el presidente de los Estados Unidos, Joe Biden.\n",
    "\"\"\"\n",
    "\n",
    "# Procesa el texto con spaCy\n",
    "doc = nlp(texto)\n",
    "\n",
    "# Extrae entidades y relaciones\n",
    "entidades = []\n",
    "relaciones = []\n",
    "for ent in doc.ents:\n",
    "    entidades.append((ent.text, ent.label_))\n",
    "    for rel in ent._.relationships:\n",
    "        relaciones.append((ent.text, rel.type, rel.target.text))\n",
    "\n",
    "# Crea una red de entidades con NetworkX\n",
    "G = nx.Graph()\n",
    "for entidad, tipo in entidades:\n",
    "    G.add_node(entidad, tipo=tipo)\n",
    "for origen, relacion, destino in relaciones:\n",
    "    G.add_edge(origen, destino, label=relacion)\n",
    "\n",
    "# Visualiza la red\n",
    "nx.draw(G, with_labels=True)\n"
