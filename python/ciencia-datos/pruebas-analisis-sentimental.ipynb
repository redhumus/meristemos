    "import spacy\n",
    "nlp = spacy.load(\"es_core_news_sm\")\n",
    "\n",
    "keywords = [\"lucha\", \"tierra\", \"campesino\", \"mujeres\", \"trabajo\", \"agua\", \"hambre\"]\n",
    "sentiments = []\n",
    "sentences = []\n",
    "\n",
    "with open('ambrosio-licona.md', 'r') as file:\n",
    "with open('ambrosio-licona.txt', 'r') as file:\n",
    "with open('ambrosio-licona.md', 'r') as file:\n",
    "    text = file.read()\n",
    "    doc = nlp(text)\n",
    "    for sentence in doc.sents:\n",
    "        for keyword in keywords:\n",
    "            if keyword in sentence.text.lower():\n",
    "                sentences.append(sentence.text)\n",
    "                polarity = TextBlob(sentence.text).sentiment.polarity\n",
    "                if polarity > 0:\n",
    "                    sentiments.append((sentence.text, \"+{:.2f}\".format(polarity)))\n",
    "                elif polarity < 0:\n",
    "                    sentiments.append((sentence.text, \"{:.2f}\".format(polarity)))\n",
    "                else:\n",
    "                    sentiments.append((sentence.text, \"{:.2f}\".format(polarity)))\n",
    "\n",
    "print(\"Frases que contienen palabras clave:\")\n",
    "print(\"\\n\".join(sentences))\n",
    "print(\"\\nValores de sentimiento:\")\n",
    "for sentence, sentiment in sentiments:\n",
    "    print(\"{}: {}\".format(sentence, sentiment))\n"
